class Solution {
    public int thirdMax(int[] nums) {
        int n=nums.length;
        Set<Integer> arr=new HashSet<>();
        for(int i=0;i<n;i++){
            arr.add(nums[i]);
        }
        List<Integer> res=new ArrayList<>(arr);
        Collections.sort(res,Collections.reverseOrder());
        if(res.size()<3){
            return res.get(0);
        }
        return res.get(2);
    }
}
