class Solution {
    public int minOperations(int[] nums, int k) {
        int n=nums.length;
        int count=0;
        for(int i=0;i<n;i++){
            count+=nums[i];
        }
        int ope=0;
        while(count%k!=0){
            count--;
            ope++;
        }
        return ope;

    }
}
